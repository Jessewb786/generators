{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "cartId": {
      "type": "string",
      "description": "Unique identifier for the cart"
    },
    "itemId": {
      "type": "string",
      "description": "Unique identifier for the checked out item"
    },
    "quantity": {
      "type": "integer",
      "minimum": 1,
      "description": "Number of items checked out"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "Time when the item was checked out"
    }
  },
  "required": ["cartId", "itemId", "quantity", "timestamp"],
  "additionalProperties": false
}
